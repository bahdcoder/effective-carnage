		"react": "^19.0.0",
		"react-dom": "^19.0.0",
		"redis": "^4.7.0",
		"sonner": "^2.0.3",
		"tailwind-merge": "^3.2.0",
		"tailwindcss": "^4.1.4",
		"zod": "^3.24.3"
	},
	"devDependencies": {
		"@types/compression": "^1.7.5",
		"@types/cookie-session": "^2.0.49",
		"@types/cors": "^2.8.17",
		"@types/express": "^5.0.0",
		"@types/node": "^22.13.4",
		"@types/react": "^19.0.10",
		"@types/react-dom": "^19.0.4",
		"@types/supertest": "^6.0.3",
		"@vitejs/plugin-react": "^4.3.4",
		"@vitest/coverage-v8": "^3.1.2",
		"cross-env": "^7.0.3",
		"prisma": "^6.6.0",
