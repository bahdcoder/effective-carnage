{
	"name": "betting-platform",
	"private": true,
	"version": "0.0.0",
	"type": "module",
	"scripts": {
		"dev": "env-cmd -f .env tsx watch core/server/dev.ts",
		"build": "npm run clean:build && npm run test:coverage && npm run lint && npm run format &&npm run build:core && npm run build:client && npm run build:server && npm run cli fix_typescript_imports",
		"build:core": "tsc",
		"clean:build": "rimraf ./build",
		"build:client": "vite build --outDir build/entry/client",
		"build:server": "vite build --ssr core/entry/server.tsx --outDir build/entry/server",
		"preview": "env-cmd -f .envnode build/server/prod.js",
		"start": "node build/server/prod.js",
		"checks:types": "tsc --noEmit",
		"checks:format": "biome format",
		"format": "biome format --write .",
		"prisma:generate": "prisma generate",
		"cli": "tsx core/cli/run.ts",
		"lint": "biome lint --write .",
		"checks:lint": "biome lint --error-on-warnings .",
		"test": "vitest",
		"test:coverage": "vitest --run --coverage"
	},
	"dependencies": {
		"@biomejs/biome": "^1.9.4",
